{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/components/(.*)",
      "dest": "/components/$1"
    },
    {
      "src": "/(.*\\.(js|jsx|css|html|png|jpg|gif|svg))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}